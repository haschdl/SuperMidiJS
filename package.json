{
   "name": "supermidijs",
   "version": "0.0.4",
   "description": "A generic wrapper for using MIDI in the browser. It's primarily target at exposing MIDI messages as pads and sliders to javascript code.\r ** Currently looking for contributors. Create an issue if you're interested in colloborating. **",
   "module": "SuperMidi.js",
   "directories": {
      "doc": "docs",
      "example": "dist/example",
      "test": "test"
   },
   "scripts": {
      "build": "rollup -c",
      "test": "nyc mocha --require @babel/register"
   },
   "presets": [
      "@babel/preset-env"
   ],
   "files": [
      "/dist",
      "/src"
   ],
   "repository": {
      "type": "git",
      "url": "git+https://github.com/haschdl/SuperMidiJS.git"
   },
   "keywords": [
      "MIDI",
      "MIDI API",
      "MIDI Controllers",
      "javascript"
   ],
   "author": "H. Scheidl <i@hscheidl.com> (https://hscheidl.com)",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/haschdl/SuperMidiJS/issues"
   },
   "homepage": "https://github.com/haschdl/SuperMidiJS#readme",
   "devDependencies": {
      "@babel/cli": "^7.0.0",
      "@babel/core": "^7.1.6",
      "@babel/polyfill": "^7.0.0",
      "@babel/preset-env": "^7.1.6",
      "@babel/register": "^7.0.0",
      "babel-preset-es2015": "^6.24.1",
      "mocha": "^5.2.0",
      "mock-local-storage": "^1.1.7",
      "nyc": "^13.1.0",
      "rollup": "^0.67.1",
      "rollup-plugin-terser": "^3.0.0"
   },
   "dependencies": {}
}
